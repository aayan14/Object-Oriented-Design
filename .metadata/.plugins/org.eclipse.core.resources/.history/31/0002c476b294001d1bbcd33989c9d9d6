package ood_intro;

public class third {
	public static void main(String[] args) {
		
		Calculator calc1 = new Calculator(4);
		calc1.add(6);
	}
}

class Calculator{
	
	int a;
	int b;
	int c;
	int res;
	
	public Calculator(int a){
		this.a = a;
	}
	
	public Calculator(int a, int b){
		this.a = a;
		this.b = b;
	}
	
	public Calculator(int a, int b, int c){
		this.a = a;
		this.b = b;
		this.c = c;
	}
	
	void add(int a){
		res = a + 1;
		System.out.println(res);
	}
	void add(int a, int b, int c){
		res = a + b + c;
		System.out.println(res);
	}
	void add(int a, int b){
		res = a + b;
		System.out.println(res);
	}
}


