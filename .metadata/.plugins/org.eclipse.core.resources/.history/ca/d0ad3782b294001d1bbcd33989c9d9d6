package ood_intro;

public class third {
	public static void main(String[] args) {
		
		Calculator calc1 = new Calculator();
		calc1.add(6);
		calc1.add(6,5);
		calc1.add(6,5,4);
	}
}

class Calculator{
	
	int a;
	int b;
	int c;
	int res;
	
	
	
	void add(){
		res = a + 1;
		System.out.println(res);
	}
	void add(int a, int b, int c){
		res = a + b + c;
		System.out.println(res);
	}
	void add(int a, int b){
		res = a + b;
		System.out.println(res);
	}
}


